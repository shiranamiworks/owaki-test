 var set_marks = function(name){
   var markerLatLng2 = new google.maps.LatLng({lat:mark_bukken.lat,lng:mark_bukken.lng});
   var marker2 = new google.maps.Marker({ 
     position: markerLatLng2,
     map: map,
     animation: google.maps.Animation.DROP,
     icon: "../static/images/location/map-logo.png"
   });
   $.each(marks[name],function(i,el){
     var markerLatLng = new google.maps.LatLng({lat:el.lat,lng:el.lng});
     marker[i] = new google.maps.Marker({ 
       position: markerLatLng,
       animation: google.maps.Animation.DROP,
       map: map,
       icon : "../static/images/location/icon-pins"+(i+1)+".png"
     });
     infoWindow[i] = new google.maps.InfoWindow({
       content: '<p class="info">' + el.label + '</p>'
     });
     marker[i].addListener('click', function() {
       $.each(marker,function(v){
         if(i !== v){
           infoWindow[v].close(map, marker[v]);
         }
       });
       infoWindow[i].open(map, marker[i]);
       map.panTo(new google.maps.LatLng(marker[i].position.lat(),marker[i].position.lng()));
     });
   });
 }


 var initMap = function(name){
   marker = [];
   infoWindow = [];
   var zoom = 16;
   switch(name){
     case "#map01" :
     zoom = 16;
     break;
     case "#map02" :
     zoom = 15;
     break;
     case "#map03" :
     zoom = 15;
     break;
     case "#map04" :
     zoom = 15;
     break;
     case "#map05" :
     zoom = 15;
     break;
     case "#map06" :
     zoom = 15;
     break;
   }
   var MyLatLng = new google.maps.LatLng(35.70722, 139.76611);
   var Options = {
     zoom: zoom,      //地図の縮尺値
     center: MyLatLng,    //地図の中心座標
     mapTypeId: 'roadmap'   //地図の種類
   };
   map = new google.maps.Map(document.getElementById('googlemap'), Options);
   set_marks(name);
 }


 $(window).on("load",function(){
   initMap("#map01");
 });

 $(".tabs a").on("click",function(e){
   e.preventDefault();
   if($(this).hasClass("current")) return;
   var t = $(this).attr("href");
   active_map = t;
   initMap(t);
   $(".tabs a").removeClass("current");
   $(this).addClass("current");
   var tt = $(this).attr("href").replace("#","");
   $(".lists").removeClass("current");
   $(".lists."+tt).addClass("current");
   set_click_func();
 });


 var set_click_func = function(){
   $("#location .list-wrapper li").off("click").on("click",function(e){
     var parent = $(this).parents(".lists");
     e.preventDefault();
     var i = parent.find("li").index(this);
     map.panTo(new google.maps.LatLng(marks[active_map][i]["lat"],marks[active_map][i]["lng"]));
     $.each(infoWindow,function(el){
       infoWindow[el].close(map, marker[el]);
     });
     infoWindow[i].open(map, marker[i]);
   });
 }
   set_click_func();
 
